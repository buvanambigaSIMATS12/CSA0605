def rearrange_array(nums):
    odd = [num for num in nums if num % 2 != 0]
    even = [num for num in nums if num % 2 == 0]
    result = []
    for i in range(len(odd)):
        result.append(odd[i])
        result.append(even[i])
    return result
nums = [1, 2, 3, 4, 5, 6]
print(rearrange_array(nums))
